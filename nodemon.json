{
    "restartable": "rs",
    "ignore": [".git", "node_modules/**/node_modules", "*.spec.js", "**/fixtures/**"],
    "verbose": true,
    "execMap": {},
    "events": {
        "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
    },
    "watch": [".", "src"],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json,ts,html,css"
}
